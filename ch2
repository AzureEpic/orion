-- Function to invoke assets by group
local function toggleAssets(group)
    for _, assetId in ipairs(group) do
        game:GetService("ReplicatedStorage").Remotes.ToggleAsset:InvokeServer(assetId)
    end
end

-- Toggle primary assets
toggleAssets(assetGroups.primary)

-- Toggle secondary assets
toggleAssets(assetGroups.secondary)

end)




